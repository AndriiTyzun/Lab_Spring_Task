<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ua.laboratory.lab_spring_task.Model.TrainingType" table="Training_Type">
        <id name="id" column="ID" type="long">
            <generator class="native"/>
        </id>

        <property name="trainingTypeName" column="Training_Type_Name" type="string"/>
    </class>
</hibernate-mapping>
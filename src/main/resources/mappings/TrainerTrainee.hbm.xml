<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ua.laboratory.lab_spring_task.Model.TrainerTrainee" table="Trainer_Trainee">
        <composite-id>
            <key-many-to-one name="trainer" class="ua.laboratory.lab_spring_task.Model.Trainer" column="Trainer_ID" />
            <key-many-to-one name="trainee" class="ua.laboratory.lab_spring_task.Model.Trainee" column="Trainee_ID" />
        </composite-id>
    </class>
</hibernate-mapping>